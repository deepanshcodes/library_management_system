{
    "name": "Django + Postgres",
    "image": "mcr.microsoft.com/devcontainers/python:3.11", 
    "features": {
      "docker-in-docker": {
        "version": "latest"
      }
    },
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "ms-azuretools.vscode-docker"
        ]
      }
    },
    "postCreateCommand": "bash .devcontainer/setup.sh",
    "forwardPorts": [8000]
  }
  